{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
, "app/utils/http.ts", "app/services/AccessToken.ts", "app/pages/Authorization/Authorization.tsx", "app/components/Wrapper/Wrapper.tsx", "app/icons/Menu.jsx", "app/components/Layout/Layout.jsx"  ],
  "exclude": [
    "node_modules"
  ]
}
